CREATE TABLE `user_table` (
  `userId` int(11) NOT NULL,
  `username` varchar(45) DEFAULT NULL,
  `password` varchar(45) DEFAULT NULL,
  `firstName` varchar(45) DEFAULT NULL,
  `lastName` varchar(45) DEFAULT NULL,
  `email` varchar(45) DEFAULT NULL,
  `userType` varchar(45) DEFAULT NULL,
  `points` int(11) DEFAULT NULL,
  PRIMARY KEY (`userId`)
)

CREATE TABLE `answer_table` (
  `answerId` int(11) NOT NULL,
  `questionId` int(11) DEFAULT NULL,
  `hint` varchar(45) DEFAULT NULL,
  `answer` varchar(45) DEFAULT NULL,
  PRIMARY KEY (`answerId`)
) 

CREATE TABLE `question_table` (
  `questionId` int(11) NOT NULL,
  `question` varchar(45) DEFAULT NULL,
  `answerId` int(45) DEFAULT NULL,
  `difficultyLevel` varchar(45) DEFAULT NULL,
  PRIMARY KEY (`questionId`)
) 

CREATE TABLE `user_question_table` (
  `idstudent_question_table` int(11) NOT NULL,
  `userId` int(11) DEFAULT NULL,
  `questionId` int(11) DEFAULT NULL,
  `noOfAttempts` int(11) DEFAULT NULL,
  `status` varchar(45) DEFAULT NULL,
  PRIMARY KEY (`idstudent_question_table`)
)
